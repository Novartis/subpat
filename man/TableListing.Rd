      list(
        dataset_name = "ADAE", variable = "c", 
        filterType = "numeric", includeMissing = FALSE, onlyMissing = FALSE, 
        createDate = "Fri Jul 26 18:44:11 2019", minVal = 1L, maxVal = 4L
